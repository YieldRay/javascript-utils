{
    "compilerOptions": {
        "target": "ESNext",
        "module": "ESNext",
        "esModuleInterop": true,
        "strict": true,
        "skipLibCheck": true,
        "allowJs": true,
        "checkJs": true,
        "noEmit": true,
        "forceConsistentCasingInFileNames": true,
        "allowImportingTsExtensions": true
    }
}
